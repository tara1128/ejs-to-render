<!-- views/pages/product.ejs -->
<% include ../partials/head %>
<body class="<%= Lang %> subpage" data-subpage="product" data-lang="<%= Lang %>">

  <div class="cmcm-wrapper">
    <section class="first-screen" id="CMCM_FirstScreen">
      <% include ../partials/header %>
    </section>

    <section class="subpage-content CMCM_SubpageContent">
      <div class="manage-width clearfix">
        <div class="submenu-nav left has-trans CMCM_SubMenus" id="CMCM_SubPageMenu_product">
          <b id="CMCM_SubMenuHeadAnchor"></b>
          <div class="category-unit">
            <a class="category-name has-trans CMCM_SubMenuItem active" href="#<%= mobileAppsLink %>"><%= mobileAppsName %></a>
            <div class="category-details CMCM_SubMenuDetails">
              <a class="category-detail-one has-trans CMCM_SMD_A active" href="#<%= mobileAppTool.hash %>"><%= mobileAppTool.name %></a>
              <a class="category-detail-one has-trans CMCM_SMD_A" href="#<%= mobileAppSocl.hash %>"><%= mobileAppSocl.name %></a>
              <a class="category-detail-one has-trans CMCM_SMD_A" href="#<%= mobileAppGame.hash %>"><%= mobileAppGame.name %></a>
            </div><!-- details -->
          </div><!-- category unit -->
          <div class="category-unit">
            <a class="category-name has-trans CMCM_SubMenuItem" href="#<%= pcLink %>"><%= pcName %></a>
          </div><!-- category unit -->
          <div class="category-unit">
            <a class="category-name has-trans CMCM_SubMenuItem" href="#<%= aiLink %>"><%= aiName %></a>
          </div><!-- category unit -->
          <div class="category-unit">
            <a class="category-name has-trans CMCM_SubMenuItem" href="#<%= bigDataLink %>"><%= bigDataName %></a>
          </div><!-- category unit -->
          <div class="category-unit">
            <a class="category-name has-trans CMCM_SubMenuItem" href="#<%= businessLink %>"><%= businessName %></a>
          </div><!-- category unit -->
          <% if (hardware) { %>
            <div class="category-unit">
              <a class="category-name has-trans CMCM_SubMenuItem" href="#<%= hardware.categoryLink %>"><%= hardware.categoryName %></a>
            </div><!-- category unit -->
          <% } %>
          <b id="CMCM_SubMenuBottomAnchor"></b>
        </div>
        <div class="sublist-container right" id="CMCM_ProductContents">

          <div class="category-container CMCM_CategoryContainer <%= mobileAppTool.anch %>" id="<%= mobileAppTool.hash %>">
            <h2 class="category-title"><%= mobileAppTool.name %></h2>
            <% mobileAppTool.data.forEach(function(app, i){ %>
              <div class="one-app-introduction clearfix">
                <div class="sub-appicon">
                  <a class="sub-prod-icon-a has-trans" href="<%= app.link %>" target="<%= app.target %>">
                    <img src="<%= app.icon %>" alt="<%= app.name %>" />
                  </a>
                </div>
                <div class="sub-appinfo">
                  <h3 class="clearfix">
                    <a class="has-trans" href="<%= app.link %>" target="<%= app.target %>"><%= app.name %></a>
                  </h3>
                  <% app.descForProd.forEach(function(text){ %>
                    <p><%= text %></p>
                  <% }); %>
                </div><!-- appinfo -->
              </div><!-- one app introduction -->
            <% }); %>
          </div><!-- end of category container -->

          <div class="category-container CMCM_CategoryContainer <%= mobileAppSocl.anch %>" id="<%= mobileAppSocl.hash %>">
            <h2 class="category-title"><%= mobileAppSocl.name %></h2>
            <% mobileAppSocl.data.forEach(function(app, i){ %>
              <div class="one-app-introduction clearfix">
                <div class="sub-appicon">
                  <a class="sub-prod-icon-a has-trans" href="<%= app.link %>" target="<%= app.target %>">
                    <img src="<%= app.icon %>" alt="<%= app.name %>" />
                  </a>
                </div>
                <div class="sub-appinfo">
                  <h3 class="clearfix">
                    <a class="has-trans" href="<%= app.link %>" target="<%= app.target %>"><%= app.name %></a>
                  </h3>
                  <% app.descForProd.forEach(function(text){ %>
                    <p><%= text %></p>
                  <% }); %>
                </div><!-- appinfo -->
              </div><!-- one app introduction -->
            <% }); %>
          </div><!-- end of category container -->

          <div class="category-container CMCM_CategoryContainer <%= mobileAppGame.anch %>" id="<%= mobileAppGame.hash %>">
            <h2 class="category-title"><%= mobileAppGame.name %></h2>
            <% mobileAppGame.data.forEach(function(app, i){ %>
              <div class="one-app-introduction clearfix">
                <div class="sub-appicon">
                  <a class="sub-prod-icon-a has-trans" href="<%= app.link %>" target="<%= app.target %>">
