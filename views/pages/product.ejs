            <% }); %>
          </div><!-- end of category container -->

          <div class="category-container CMCM_CategoryContainer <%= bigDataData.bigData.anch %>" id="<%= bigDataData.bigData.hash %>">
            <h2 class="category-title"><%= bigDataData.bigData.name %></h2>
