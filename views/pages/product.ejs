              </div><!-- one app introduction -->
            <% }); %>
          </div><!-- end of category container -->

          <div class="category-container CMCM_CategoryContainer <%= businessData.business.anch %>" id="<%= businessData.business.hash %>">
            <h2 class="category-title"><%= businessData.business.name %></h2>
            <% businessData.business.data.forEach(function(app, i){ %>
              <div class="one-app-introduction clearfix">
                <div class="sub-appicon">
                  <a class="sub-prod-icon-a has-trans" href="<%= app.link %>" target="<%= app.target %>">
                    <img src="<%= app.icon %>" alt="<%= app.name %>" />
                  </a>
                </div>
                <div class="sub-appinfo">
                  <h3 class="clearfix">
                    <a class="has-trans" href="<%= app.link %>" target="<%= app.target %>"><%= app.name %></a>
                  </h3>
                  <% app.descForProd.forEach(function(text){ %>
                    <p><%= text %></p>
                  <% }); %>
                </div><!-- appinfo -->
              </div><!-- one app introduction -->
            <% }); %>
          </div><!-- end of category container -->

          <% if (hardware) { %>
            <div class="category-container CMCM_CategoryContainer <%= hardware.categoryData.hardware.anch %>" id="<%= hardware.categoryData.hardware.hash %>">
              <h2 class="category-title"><%= hardware.categoryData.hardware.name %></h2>
              <% hardware.categoryData.hardware.data.forEach(function(app, i){ %>
                <div class="one-app-introduction clearfix">
                  <div class="sub-appicon">
                    <a class="sub-prod-icon-a has-trans" href="<%= app.link %>" target="<%= app.target %>">
                      <img src="<%= app.icon %>" alt="<%= app.name %>" />
                    </a>
                  </div>
                  <div class="sub-appinfo">
                    <h3 class="clearfix">
                      <a class="has-trans" href="<%= app.link %>" target="<%= app.target %>"><%= app.name %></a>
                    </h3>
                    <% app.descForProd.forEach(function(text){ %>
                      <p><%= text %></p>
                    <% }); %>
                  </div><!-- appinfo -->
                </div><!-- one app introduction -->
              <% }); %>
            </div><!-- end of category container -->
          <% } %>
        </div><!-- product contents -->
      </div><!-- manage width -->
      <b id="CMCM_SubPageContentBottom"></b>
    </section><!-- subpage content -->

    <% include ../partials/footer %>
  </div><!-- .cmcm-wrapper -->
  <script src="/dist/index.min.js?v=12271226"></script>
</body>
</html>
